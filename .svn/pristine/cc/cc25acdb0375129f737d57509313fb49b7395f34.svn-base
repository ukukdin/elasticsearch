        <typeAlias type="nurier.scraping.common.vo.MenuDataVO"           alias="menuDataVO"         />
        <typeAlias type="nurier.scraping.common.vo.GroupDataVO"          alias="groupDataVO"        />
        <typeAlias type="nurier.scraping.common.vo.DashBoardDataVO"      alias="dashBoardDataVO"    />
        <typeAlias type="nurier.scraping.common.vo.CodeDataVO"           alias="codeDataVO"         />
        <typeAlias type="nurier.scraping.common.vo.QueryVO"              alias="queryVO"            />
