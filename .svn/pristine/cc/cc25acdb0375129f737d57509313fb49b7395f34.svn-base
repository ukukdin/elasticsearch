<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
        <typeAlias type="nurier.scraping.common.vo.ReportVO"             alias="reportVO"           />
        <typeAlias type="nurier.scraping.common.vo.AuthGroupVO"          alias="authGroupVO"        />
        <typeAlias type="nurier.scraping.common.vo.BlackUserVO"          alias="blackUserVO"        />
        <typeAlias type="nurier.scraping.common.vo.UserAuthVO"           alias="userAuthVO"         />
