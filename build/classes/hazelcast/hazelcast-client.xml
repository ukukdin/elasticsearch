<hazelcast-client xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.hazelcast.com/schema/client-config 
                               http://hazelcast.com/schema/client-config/hazelcast-client-config-3.7.xsd"
           xmlns="http://www.hazelcast.com/schema/client-config">
           
    <instance-name>antiscraping-adminWebserver</instance-name>
           
    <cluster-name>nurier-hazelcast</cluster-name>

    <network>
        <cluster-members>
            <address>192.168.40.104:32000</address>
            <address>192.168.40.121:32000</address>
        </cluster-members>
    </network>
    
    <properties>
        <property name="hazelcast.logging.type">log4j</property>
    </properties>
</hazelcast-client>
